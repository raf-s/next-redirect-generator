{
  "name": "nextjs-to-netlify-redirect-exporter",
  "version": "2.0.9",
  "author": "Raf Sz. <raf.sz@icloud.com>",
  "license": "MIT",
  "main": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:raf-s/nextjs-to-netlify-redirect-exporter.git"
  },
  "bugs": "https://github.com/raf-s/nextjs-to-netlify-redirect-exporter/issues",
  "description": "CLI tool to generate netlify.toml redirects from Next.js dynamic routes in pages folder.",
  "keywords": [
    "next.js",
    "next",
    "static export",
    "dynamic routes",
    "pages folder",
    "ssg",
    "static site generation",
    "netlify",
    "redirect",
    "redirects",
    "rewrite",
    "rewrites"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "tsup src/index.ts --onSuccess \"yarn link:self\"",
    "lint": "eslint --fix",
    "type-check": "tsc",
    "link:self": "yarn yalc publish --push",
    "release": "yarn build && yarn release-it"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "bin": {
    "nextjs-to-netlify-redirect-exporter": "bin/nextjs-to-netlify-redirect-exporter.js"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.7",
    "release-it": "^17.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "nextjs-to-netlify-redirect-exporter": "file:.yalc/nextjs-to-netlify-redirect-exporter",
    "yargs": "^17.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.{json,gql,graphql,yaml,yml}": [
      "prettier --write"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "comments": true
    },
    "npm": {
      "publish": false
    }
  }
}
